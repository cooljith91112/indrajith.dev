---
layout: base.njk
eleventyExcludeFromCollections: true
title: Posts
---
<h2>Posts</h2>
<div class="post-lists">
    {% for post in collections.posts %}
        <div class="post-data">
            <div class="post-date">
                {{post.date | dateFilter}}
            </div>
            <div class="post-title color-red">
               <a href="{{post.url}}"> {{post.data.title}}</a> 
            </div>
        </div>
    
    {% endfor %}
</div>

